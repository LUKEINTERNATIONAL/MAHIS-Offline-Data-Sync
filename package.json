{
    "name": "nest-payload-receiver",
    "version": "1.0.0",
    "description": "NestJS API for receiving payloads",
    "main": "dist/main.js",
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "start": "ts-node src/main.ts",
        "start:dev": "ts-node-dev --respawn src/main.ts",
        "start:prod": "node dist/main.js",
        "test": "jest",
        "schema:generate": "node scripts/generate-schema.js",
        "prisma:generate": "npm run schema:generate && prisma generate",
        "dev": "npm run prisma:generate && npm run start:dev"
    },
    "prisma": {
        "schema": "src/modules/prisma/schema.prisma"
    },
    "keywords": [
        "nestjs",
        "api",
        "typescript",
        "payload"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@nestjs/axios": "^4.0.0",
        "@nestjs/common": "^10.2.0",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^10.2.0",
        "@nestjs/platform-express": "^10.2.0",
        "@nestjs/platform-socket.io": "^10.4.17",
        "@nestjs/schedule": "^6.0.0",
        "@nestjs/swagger": "^11.2.0",
        "@nestjs/websockets": "^10.4.17",
        "@prisma/client": "^6.10.1",
        "@types/node": "^18.17.5",
        "axios": "^1.9.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "node-forge": "^1.3.1",
        "node-gyp": "^11.2.0",
        "node-localstorage": "^3.0.5",
        "prisma": "^6.10.1",
        "qrcode": "^1.5.4",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.2",
        "socket.io": "^4.8.1",
        "sqlite3": "^5.1.7",
        "swagger-ui-express": "^5.0.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.1.17",
        "@nestjs/testing": "^10.2.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.3",
        "jest": "^29.6.2",
        "ts-jest": "^29.1.1",
        "ts-node-dev": "^2.0.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
